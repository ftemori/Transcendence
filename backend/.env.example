# Backend environment variables for blockchain integration
# Copy this file to `.env` and fill in the values. Do NOT commit your real secrets.

# Avalanche RPC endpoint (use Fuji testnet for development)
# Example: https://api.avax-test.network/ext/bc/C/rpc
AVAX_RPC_URL=

# Private key of the deployer/owner account (0x-prefixed hex)
# This must be the account that deployed the TournamentScores contract
PRIVATE_KEY=

# Deployed TournamentScores contract address (0x...)
TOURNAMENT_CONTRACT_ADDRESS=
